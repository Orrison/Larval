{
  "name": "larval",
  "productName": "Laraval",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bulma": "^0.7.4",
    "bulma-tooltip": "^2.0.2",
    "electron": "^4.0.5",
    "electron-settings": "git+https://github.com/Orrison/electron-settings-react.git",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "fix-path": "^2.1.0",
    "jquery": "^3.3.1",
    "js-yaml": "^3.12.1",
    "line-by-line": "git+https://github.com/Orrison/line-by-line.git",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "^2.1.5",
    "sudo-prompt": "git+https://github.com/Orrison/sudo-prompt-react-electron.git",
    "time-stamp": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "BROWSER=none react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "install-app-deps",
    "package-mac": "npm run-script build && electron-packager . --overwrite --icon=icons/larva.icns --out=dist"
